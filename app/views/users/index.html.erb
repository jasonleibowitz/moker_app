<div class="row">
  <div class="small-space"></div>
</div>
<div class="row">
  <div class="small-4 small-centered columns">
    <h1>Moker Baby!</h1>
  </div>
</div>

<div class="row">
  <div class="small-6">
    <% if current_user %>
    <div class="avatar-wrap">
      <img src="<%= "#{current_user.image}" %>?type=large" alt="">
    </div>
    <p>You are currently logged in as <%= current_user.email %>. <%= link_to "Log Out", destroy_user_session_path, method: :delete %></p>
    <% else %>
      <a href="<%= user_omniauth_authorize_path(:facebook) %>"> <%= image_tag("fbooksign-in.png") %></a>
    <% end %>
  </div>
</div>

